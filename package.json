		"eslint-plugin-storybook": "^0.6.15",
		"gh-pages": "^6.1.0",
		"husky": "^8.0.0",
		"lint-staged": "^14.0.1",
		"prettier": "3.0.3",
		"prop-types": "^15.8.1",
		"sass": "^1.69.2",
		"storybook": "^7.4.6",
		"webpack": "^5.88.2"
	},
	"lint-staged": {
		"*.{js,jsx}": "npm run lint",
		"*.{js,jsx,css,md,html,json}": "npm run format"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
